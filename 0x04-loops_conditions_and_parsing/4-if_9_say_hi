#!/usr/bin/env bash
#Displays Best School 10 times using while loop.
#on the 9th iteration, displays "Hi" before "Best School".

count=1
while [ $count -le 10 ]; do
    if [ $count -eq 9 ]; then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi
    count=$((count + 1))
done
